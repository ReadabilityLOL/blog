      <description>&lt;style&gt;&#xA;.about{&#xA;  width:200%;&#xA;}&#xA;.contrib-chart{&#xA;  /*border: 3px solid white;*/&#xA;  transform: translate(0px, 2ch);&#xA;  scale: 1.5;&#xA;  contain: layout;&#xA;  margin-left: 10%;&#xA;}&#xA;&#xA;/*&#xA;.stats-label:before,&#xA;.stats-label:after {&#xA;    content: &#34;&#34;;&#xA;    flex: 1 1;&#xA;    border-bottom: 2px solid #ffffff;&#xA;    margin: auto;&#xA;}&#xA;*/&#xA;&#xA;.stats-label {&#xA;  margin: 0 auto;&#xA;  /*display: flex;&#xA;  flex-direction: row;*/&#xA;}&#xA;&#xA;.stats-container {&#xA;  margin-top: 15ch;&#xA;  display: flex;&#xA;  flex-wrap: wrap;&#xA;}&#xA;&#xA;.stat {&#xA;  border-radius: 2px;&#xA;  border: 3px solid #ffffff;&#xA;  flex: 20%;&#xA;}&#xA;&#xA;table{&#xA;  border: 3px solid #ffffff;&#xA;  margin-bottom:1em;&#xA;}&#xA;&#xA;th, td{&#xA;  width:25%;&#xA;  text-align: center;&#xA;}&#xA;&#xA;&lt;/style&gt;&#xA;&#xA;&lt;center&gt;&lt;h1 class=&#34;stats-label&#34;&gt;My stats&lt;/h1&gt;&lt;/center&gt;&#xA;&lt;hr&gt;&#xA;&lt;h3&gt; Github contributions&#xA;&lt;center&gt;&lt;img class = &#34;contrib-chart&#34; src=&#34;http://ghchart.rshah.org/readabilityLOL&#34; alt=&#34;My github contributions chart&#34;&gt;&lt;/center&gt;&#xA;&#xA;&lt;center class = &#34;stats-container&#34;&gt;&#xA;&lt;div id = &#34;wca&#34; class = &#34;wca stat&#34;&gt; my wca 3x3 pr here &lt;/div&gt;&#xA;&lt;div id = &#34;elo&#34; class = &#34;stat&#34;&gt; lichess stats here &lt;/div&gt;&#xA;&lt;/center&gt;&#xA;&#xA;&lt;script&gt;&#xA;let elo_container = document.getElementById(&#34;elo&#34;)&#xA;let wca_container = document.getElementById(&#34;wca&#34;)&#xA;&#xA;//curl --location &#39;https://raw.githubusercontent.com/robiningelbrecht/wca-rest-api/master/api/persons.json&#39;&#xA;async function getData() {&#xA;  const url = &#39;https://lichess.org/api/user/JimKram&#39;;&#xA;  try {&#xA;    const response = await fetch(url);&#xA;    if (!response.ok) {&#xA;      throw new Error(`Response status: ${response.status}`);&#xA;    }&#xA;&#xA;    const result = await response.json();&#xA;    return result&#xA;  } catch (error) {&#xA;    console.error(error.message);&#xA;  }&#xA;}&#xA;&#xA;getData().then((promisedata) =&gt; {&#xA;  perfs = promisedata[&#34;perfs&#34;]&#xA;  console.log(promisedata)&#xA;  elo_container.innerHTML = `&#xA;&lt;div style=&#34;border-bottom: 3px solid #ffffff; margin-bottom: 1em;&#34;&gt;Lichess&lt;/div&gt;&#xA;&lt;table&gt;&#xA;  &lt;tr&gt;&#xA;    &lt;th&gt;&lt;/th&gt;&#xA;    &lt;th&gt;Bullet&lt;/th&gt;&#xA;    &lt;th&gt;Blitz&lt;/th&gt;&#xA;    &lt;th&gt;Rapid&lt;/th&gt;&#xA;  &lt;/tr&gt;&#xA;  &lt;tr&gt;&#xA;    &lt;th&gt;Games&lt;/th&gt;&#xA;    &lt;td&gt;${perfs[&#34;bullet&#34;][&#34;games&#34;]}&lt;/td&gt;&#xA;    &lt;td&gt;${perfs[&#34;blitz&#34;][&#34;games&#34;]}&lt;/td&gt;&#xA;    &lt;td&gt;${perfs[&#34;rapid&#34;][&#34;games&#34;]}&lt;/td&gt;&#xA;  &lt;/tr&gt;&#xA;  &lt;tr&gt;&#xA;    &lt;th&gt;Rating&lt;/th&gt;&#xA;    &lt;td&gt;${perfs[&#34;bullet&#34;][&#34;rating&#34;]}&lt;/td&gt;&#xA;    &lt;td&gt;${perfs[&#34;blitz&#34;][&#34;rating&#34;]}&lt;/td&gt;&#xA;    &lt;td&gt;${perfs[&#34;rapid&#34;][&#34;rating&#34;]}&lt;/td&gt;&#xA;  &lt;/tr&gt;&#xA;&lt;/table&gt;&#xA;  `&#xA;})&#xA;&#xA;wca_container.innerHTML = `&#xA;&lt;div style=&#34;border-bottom: 3px solid #ffffff; margin-bottom: 1em;&#34;&gt;Cubing&lt;/div&gt;&#xA;&lt;table&gt;&#xA;  &lt;tr&gt;&#xA;    &lt;th&gt;&lt;/th&gt;&#xA;    &lt;th&gt;3x3&lt;/th&gt;&#xA;    &lt;th&gt;4x4&lt;/th&gt;&#xA;    &lt;th&gt;Skewb&lt;/th&gt;&#xA;  &lt;/tr&gt;&#xA;  &lt;tr&gt;&#xA;    &lt;th&gt;Single&lt;/th&gt;&#xA;    &lt;td&gt;${17.68}&lt;/td&gt;&#xA;    &lt;td&gt;${&#34;n/a&#34;}&lt;/td&gt;&#xA;    &lt;td&gt;${19.95}&lt;/td&gt;&#xA;  &lt;/tr&gt;&#xA;  &lt;tr&gt;&#xA;    &lt;th&gt;Ao12&lt;/th&gt;&#xA;    &lt;td&gt;${23.01}&lt;/td&gt;&#xA;    &lt;td&gt;${&#34;n/a&#34;}&lt;/td&gt;&#xA;    &lt;td&gt;${24.28}&lt;/td&gt;&#xA;  &lt;/tr&gt;&#xA;&lt;/table&gt;`&#xA;&#xA;&lt;/script&gt;</description>
    </item>
